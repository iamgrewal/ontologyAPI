CREATE TABLE JobRequest (
 JobID INT NOT NULL AUTO_INCREMENT,
 UserID INT NOT NULL,
 JobTitle VARCHAR(255) NOT NULL,
 MatchedTitle VARCHAR(255) NOT NULL,
 StartDate DATE NOT NULL,
 EndDate DATE NOT NULL,
 ReqDateHour DATETIME NOT NULL,
 Language VARCHAR(255) NOT NULL,
 PRIMARY KEY (JobID));


 CREATE TABLE SkillResponse (
 SkillsID INT NOT NULL AUTO_INCREMENT,
 JobID INT NOT NULL,
 Skill VARCHAR(255) NOT NULL,
 IsRight INT NOT NULL,
 PRIMARY KEY (SkillsID),
 FOREIGN KEY (JobID) REFERENCES JobRequest(JobID)
 );



